{
    "name": "mingalevme/opcache-status-info",
    "description": "PHP OPcache status information object",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Mingalev",
            "email": "mingalevme@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12.92"
    },
    "autoload": {
        "psr-4": {
            "Mingalevme\\OpcacheStatusInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mingalevme\\Tests\\OpcacheStatusInfo\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "psalm": "psalm",
        "phpstan": "phpstan analyze",
        "phpcs": "phpcs"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
